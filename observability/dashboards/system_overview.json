{
  "id": null,
  "uid": "wotr-system-overview",
  "title": "WOTR - System Overview",
  "tags": ["wotr", "overview", "system"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "row",
      "title": "Platform Health",
      "id": 100,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "collapsed": false
    },
    {
      "type": "gauge",
      "title": "Overall System Health",
      "id": 1,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "((postgres_status>0) + (minio_status>0) + (kafka_status>0) + (clickhouse_status>0) + (app_uptime>0)) / 5 * 100",
          "legendFormat": "Health %",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "orange", "value": 60},
              {"color": "yellow", "value": 80},
              {"color": "green", "value": 95}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 1}
    },
    {
      "type": "stat",
      "title": "Service Status",
      "id": 2,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {"expr": "postgres_status", "legendFormat": "PostgreSQL", "refId": "A"},
        {"expr": "minio_status", "legendFormat": "MinIO", "refId": "B"},
        {"expr": "kafka_status", "legendFormat": "Kafka", "refId": "C"},
        {"expr": "clickhouse_status", "legendFormat": "ClickHouse", "refId": "D"}
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "name",
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "green", "value": 1}
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {"text": "DOWN", "color": "red"},
                "1": {"text": "UP", "color": "green"}
              }
            }
          ]
        }
      },
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 1}
    },
    {
      "type": "stat",
      "title": "FastAPI Uptime",
      "id": 3,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "app_uptime",
          "legendFormat": "Uptime",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "yellow", "value": null},
              {"color": "green", "value": 300}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 1}
    },
    {
      "type": "row",
      "title": "Request Metrics",
      "id": 101,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Request Rate & Errors",
      "id": 4,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "rate(fastapi_requests_total[1m])",
          "legendFormat": "Requests/sec",
          "refId": "A"
        },
        {
          "expr": "rate(fastapi_error_total[1m])",
          "legendFormat": "Errors/sec",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {"lineWidth": 2, "fillOpacity": 10, "showPoints": "never"}
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Errors/sec"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          }
        ]
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10}
    },
    {
      "type": "timeseries",
      "title": "P95 Latency",
      "id": 5,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(fastapi_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P95 Latency",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {"lineWidth": 2, "fillOpacity": 10}
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10}
    },
    {
      "type": "row",
      "title": "Data Pipeline",
      "id": 102,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18},
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Total Ingests (5m)",
      "id": 6,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "increase(wotr_ingest_total[5m])",
          "legendFormat": "Ingests",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "blue", "value": null}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 19}
    },
    {
      "type": "timeseries",
      "title": "Ingest Rate Over Time",
      "id": 7,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "rate(wotr_ingest_total[1m])",
          "legendFormat": "Ingest Rate (ops/sec)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {"lineWidth": 2, "fillOpacity": 10}
        }
      },
      "gridPos": {"h": 6, "w": 16, "x": 8, "y": 19}
    },
    {
      "type": "row",
      "title": "Alerts",
      "id": 103,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 25},
      "collapsed": false
    },
    {
      "type": "text",
      "title": "Active Alerts",
      "id": 8,
      "options": {
        "content": "Check **Alertmanager** at [http://localhost:9093](http://localhost:9093) for active alerts.\n\nConfigured alerts:\n- FastAPI High Latency\n- FastAPI Errors\n- Service Down (Postgres, ClickHouse, Kafka, MinIO)\n- High CPU Usage\n- High Error Rate\n- Request Spike",
        "mode": "markdown"
      },
      "gridPos": {"h": 6, "w": 24, "x": 0, "y": 26}
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "query": "prometheus",
        "current": {"text": "Prometheus", "value": "Prometheus"},
        "hide": 0
      }
    ]
  },
  "annotations": {"list": []},
  "time": {"from": "now-1h", "to": "now"},
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m"],
    "time_options": ["15m", "1h", "6h", "12h", "24h"]
  },
  "links": [
    {
      "title": "FastAPI Detailed",
      "url": "/d/wotr-fastapi-detailed",
      "type": "link"
    },
    {
      "title": "Kafka & Consumer",
      "url": "/d/wotr-kafka-consumer",
      "type": "link"
    }
  ]
}
