{
  "id": null,
  "uid": "wotr-fastapi-detailed",
  "title": "WOTR - FastAPI Detailed Metrics",
  "tags": ["wotr", "fastapi", "api"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "row",
      "title": "Request Overview",
      "id": 100,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Total Requests (5m)",
      "id": 1,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "increase(fastapi_requests_total[5m])",
          "legendFormat": "Requests",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 500}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 4, "x": 0, "y": 1}
    },
    {
      "type": "stat",
      "title": "Request Rate (req/s)",
      "id": 2,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "rate(fastapi_requests_total[1m])",
          "legendFormat": "req/s",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 50}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 4, "x": 4, "y": 1}
    },
    {
      "type": "stat",
      "title": "Error Rate (%)",
      "id": 3,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "rate(fastapi_error_total[5m]) / rate(fastapi_requests_total[5m]) * 100",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 5}
            ]
          },
          "noValue": "0"
        }
      },
      "gridPos": {"h": 6, "w": 4, "x": 8, "y": 1}
    },
    {
      "type": "stat",
      "title": "Avg Latency (5m)",
      "id": 4,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "rate(fastapi_request_duration_seconds_sum[5m]) / rate(fastapi_request_duration_seconds_count[5m])",
          "legendFormat": "Avg Latency",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 0.5},
              {"color": "red", "value": 1.0}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 4, "x": 12, "y": 1}
    },
    {
      "type": "stat",
      "title": "P95 Latency (5m)",
      "id": 5,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(fastapi_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P95",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1.0},
              {"color": "red", "value": 2.0}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 4, "x": 16, "y": 1}
    },
    {
      "type": "stat",
      "title": "Ingest Operations (5m)",
      "id": 6,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "increase(wotr_ingest_total[5m])",
          "legendFormat": "Ingests",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "blue", "value": null}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 4, "x": 20, "y": 1}
    },
    {
      "type": "row",
      "title": "Request Trends",
      "id": 101,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 7},
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Request Rate Over Time",
      "id": 7,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "rate(fastapi_requests_total[1m])",
          "legendFormat": "Requests/sec",
          "refId": "A"
        },
        {
          "expr": "rate(fastapi_error_total[1m])",
          "legendFormat": "Errors/sec",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {"lineWidth": 2, "fillOpacity": 10, "showPoints": "never"}
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Errors/sec"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          }
        ]
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "type": "timeseries",
      "title": "Latency Percentiles",
      "id": 8,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(fastapi_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, rate(fastapi_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, rate(fastapi_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P95",
          "refId": "C"
        },
        {
          "expr": "histogram_quantile(0.99, rate(fastapi_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P99",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {"lineWidth": 2, "fillOpacity": 0}
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    },
    {
      "type": "row",
      "title": "Database & Backend Services",
      "id": 102,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 16},
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Database Query Latency",
      "id": 9,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "rate(fastapi_db_query_duration_seconds_sum[5m]) / rate(fastapi_db_query_duration_seconds_count[5m])",
          "legendFormat": "Avg DB Latency",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {"lineWidth": 2, "fillOpacity": 10}
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 17}
    },
    {
      "type": "stat",
      "title": "Service Status Summary",
      "id": 10,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {"expr": "postgres_status", "legendFormat": "Postgres", "refId": "A"},
        {"expr": "clickhouse_status", "legendFormat": "ClickHouse", "refId": "B"},
        {"expr": "kafka_status", "legendFormat": "Kafka", "refId": "C"},
        {"expr": "minio_status", "legendFormat": "MinIO", "refId": "D"}
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "name"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "green", "value": 1}
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {"text": "DOWN", "color": "red"},
                "1": {"text": "UP", "color": "green"}
              }
            }
          ]
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 17}
    },
    {
      "type": "row",
      "title": "Detailed Metrics",
      "id": 103,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 25},
      "collapsed": false
    },
    {
      "type": "table",
      "title": "Recent Errors",
      "id": 11,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "rate(fastapi_error_total[5m])",
          "legendFormat": "{{job}}",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26}
    },
    {
      "type": "timeseries",
      "title": "Application Uptime",
      "id": 12,
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "expr": "app_uptime",
          "legendFormat": "Uptime (seconds)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {"lineWidth": 2, "fillOpacity": 10}
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26}
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "query": "prometheus",
        "current": {"text": "Prometheus", "value": "Prometheus"},
        "hide": 0
      }
    ]
  },
  "annotations": {"list": []},
  "time": {"from": "now-1h", "to": "now"},
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h"]
  },
  "links": []
}
